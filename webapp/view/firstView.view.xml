<mvc:View controllerName="promos.exad.EXAD2.controller.firstView" 
	xmlns="sap.m" 
	xmlns:t="sap.ui.table"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:uxap="sap.uxap"
	xmlns:tnt="sap.tnt"
	xmlns:l="sap.ui.layout" 
	xmlns:core="sap.ui.core">
	<Panel id="gridLayout" width="100%" height="100%">
		<headerToolbar>
			<OverflowToolbar height="5rem">
				<l:HorizontalLayout class="sapUiContentPadding">
					<Input id="KundeSearch" type="Text" class="sapUiSmallMarginBottom" placeholder="Kunde"/>
					<Input id="LiegenschaftSearch" type="Tel" class="sapUiSmallMarginBottom" placeholder="Liegenschaft"/>
					<DateRangeSelection id="DateRangeSearch" change="handleChange"/>
					<Button text="Auswahl laden" press="onPress"/>
				</l:HorizontalLayout>
			</OverflowToolbar>
		</headerToolbar>
		<IconTabBar
				items="{path: '/dienst' }"
				id="iconTabBar"
				enableTabReordering="true"
				headerBackgroundDesign="Translucent"
				expandable="false"
				class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter	text="{title}">
						<t:Table	rows="{path: '/tableDetails'}"
									selectionMode="MultiToggle"
									visibleRowCount="5"
									paste="onPaste">
							<t:columns name="{path: '/columnData' }" >
						
							
							</t:columns>
						</t:Table>
					</IconTabFilter>
				</items>
			</IconTabBar>
	</Panel>
</mvc:View>