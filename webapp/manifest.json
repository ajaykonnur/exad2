{
	"_version": "1.12.0",
	"sap.app": {
		"id": "promos.exad.EXAD2",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "promos.exad.EXAD2.view.main.main",
			"type": "XML",
			"async": true,
			"id": "main"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "promos.exad.EXAD2.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "promos.exad.EXAD2.view",
				"controlAggregation": "pages",
				"controlId": "appContainer",
				"clearControlAggregation": false
			},
		"routes": [
				{
					"pattern": "",
					"name": "homeRoute",
					"target": "billingProcess"
				},{
					"pattern": "billingProcess",
					"name": "billingProcessRoute",
					"target": "billingProcess"
				},{
					"pattern": "dataSet",
					"name": "dataSetRoute",
					"target": "dataSet"
				},{
					"pattern": "dataExchange",
					"name": "dataExchangeRoute",
					"target": "dataExchange"
				},{
					"pattern": "analysis",
					"name": "analysisRoute",
					"target": "analysis"
				},{
					"pattern": "deviceManagement",
					"name": "deviceManagementRoute",
					"target": "deviceManagement"
				},{
					"pattern": "parameters",
					"name": "parametersRoute",
					"target": "parameters"
				}],
		"targets":{
			"billingProcess":{
				"viewName": "targets/billingProcess/billingProcess",
				"viewId": "billingProcess"
				},
			"dataSet":{
					"viewName": "targets/dataSet",
					"viewId": "dataSet"
				},
			"dataExchange":{
					"viewName": "targets/dataExchange",
					"viewId": "dataExchange"
				},
			"analysis":{
					"viewName": "targets/analysis",
					"viewId": "analysis"
				},
			"deviceManagement":{
					"viewName": "targets/deviceManagement",
					"viewId": "deviceManagement"
				},
			"parameters":{
					"viewName": "targets/parameters",
					"viewId": "parameters"
				}
			}
		}
	}
}