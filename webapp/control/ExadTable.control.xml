<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table">
	<t:Table columns="{/columnData}" rows="{/tableDetails}" selectionMode="MultiToggle" visibleRowCount="7" paste="onPaste">
		<t:columns>
			<t:Column></t:Column>
		</t:columns>
		<t:extension>
			<Toolbar class="sapMTBHeader-CTX">
				<Title text="{$this>title}"/>
				<ComboBox selectedKey="default">
					<items>
						<core:Item key="default" text="Standard"/>
					</items>
				</ComboBox>
				<Button text="Funktion"/>
				<ToolbarSpacer/>
				<Label text="bearbeiten"/>
				<Switch change=".onEditChange"/>
				<Button tooltip="Neu" icon="sap-icon://add" type="Accept" enabled="{$this>editable}" press=".onAddRowPress"/>
				<Button tooltip="Kopieren" icon="sap-icon://copy" enabled="{$this>editable}" press=".onCopyRowPress"/>
				<Button tooltip="LÃ¶schen" icon="sap-icon://delete" type="Reject" enabled="{$this>editable}" press=".onDeleteRowPress"/>
			</Toolbar>
		</t:extension>
	</t:Table>
</core:FragmentDefinition>