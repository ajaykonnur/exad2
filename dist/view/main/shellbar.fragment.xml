<core:FragmentDefinition xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:core="sap.ui.core">
	<tnt:ToolHeader class="myCustomClass" id="shellbar">
		<ToggleButton id="idFunctionsToggleButton" icon="sap-icon://menu2" pressed="false" type="Transparent" press=".onFunctionsToggleButtonPress">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow"/>
			</layoutData>
		</ToggleButton>
		<ToolbarSeparator/>
		<Image src="resources/main_logo_.PNG" width="17em"  />
		<ToolbarSpacer/>
		<Button type="Transparent" icon="sap-icon://home">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low"/>
			</layoutData>
		</Button>
		<Button type="Transparent" icon="sap-icon://activity-items">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low"/>
			</layoutData>
		</Button>
		<Button type="Transparent" icon="sap-icon://favorite">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low"/>
			</layoutData>
		</Button>
		<Button type="Transparent" icon="sap-icon://log">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low"/>
			</layoutData>
		</Button>
		<ToolbarSeparator/>
		<ToggleButton id="idSidebarToggleButton" icon="sap-icon://vertical-grip" pressed="false" type="Transparent" press=".onSidebarToggleButtonPress">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow"/>
			</layoutData>
		</ToggleButton>
	</tnt:ToolHeader>
</core:FragmentDefinition>