	        	<Button text="Als Kopie speichern" press="onPress" />
	        	<Button text="Löschen" press="onPress" />
	        </uxap:expandedContent>
        </uxap:ObjectPageDynamicHeaderTitle>
    </uxap:headerTitle>
    <uxap:headerContent>
    </uxap:headerContent>
    <uxap:sections>
    	<uxap:ObjectPageSection id="Section_1" title="Section 1">
        	<uxap:subSections>
            	<uxap:ObjectPageSubSection id="SectionSS1" title=" ">
                	<uxap:blocks>
                    	<Label text="Kundennummer" />
                    	<Input id="Kundennummer" />
                    		<Label text="Liegenschaftsnummer" />
                    	<Input id="Liegenschaftsnummer" />
                    		<Label text="Bezeichner" />
                    	<Input id="Bezeichner" />
                    		<Label text="Straße" />
                    	<Input id="Strasse" />
                    		<Label text="Postleitzahl" />
                    	<Input id="Postleitzahl" />
                    		<Label text="Ort" />
                    	<Input id="Ort" />
                    </uxap:blocks>
                </uxap:ObjectPageSubSection>
            </uxap:subSections>
        </uxap:ObjectPageSection>
    	<uxap:ObjectPageSection id="Section_2" title="Section 2">
        	<uxap:subSections>
            	<uxap:ObjectPageSubSection id="SectionSS2" title=" ">
                	<uxap:blocks>
                    		<Label text="Postleitzahl" />
