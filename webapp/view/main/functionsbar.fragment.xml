<core:FragmentDefinition xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:core="sap.ui.core"
	xmlns:cust="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<tnt:SideNavigation expanded="false" id="idFunctionsBar">
		<tnt:NavigationList>
			<tnt:NavigationListItem select=".onSideNavigationItemSelect" cust:route="dataSetRoute" text="{i18n>dataSet}" icon="sap-icon://building"/>
			<tnt:NavigationListItem select=".onSideNavigationItemSelect" cust:route="billingProcessRoute" text="{i18n>billingProcess}"
				icon="sap-icon://checklist"/>
			<tnt:NavigationListItem select=".onSideNavigationItemSelect" cust:route="dataExchangeRoute" text="{i18n>dataExchange}"
				icon="sap-icon://sort"/>
			<tnt:NavigationListItem select=".onSideNavigationItemSelect" cust:route="analysisRoute" text="{i18n>analysis}" icon="sap-icon://bar-chart"/>
			<tnt:NavigationListItem select=".onSideNavigationItemSelect" cust:route="deviceManagementRoute" text="{i18n>deviceManagement}"
				icon="sap-icon://responsive"/>
			<tnt:NavigationListItem select=".onSideNavigationItemSelect" cust:route="parametersRoute" text="{i18n>parameters}"
				icon="sap-icon://provision"/>
		</tnt:NavigationList>
	</tnt:SideNavigation>
</core:FragmentDefinition>